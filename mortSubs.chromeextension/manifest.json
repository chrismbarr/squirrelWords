{
  "manifest_version": 2,

  "name": "mortSubs",
  "description": "Some fun replacements, many stolen from XKCD.",
  "version": "1.2",
  "permissions": [
    "tabs", "http://*/*, https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["global.js"]
    }
  ]
}
